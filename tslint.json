{
  "extends": "gts/tslint.json",
  "linterOptions": {
    "exclude": [
      "src/app/frontend/environments/version.ts"
    ]
  },
  "rules": {
    "ban-ts-ignore": false,
    "interface-over-type-literal": false,
    "deprecation": false,
    "trailing-comma": [
      true,
      {
        "multiline": "always"
      }
    ],
    "no-any": false
  }
}
